			function search() {
				$(window).ready(function(){
					$('#searchfullscreen').trigger('click')
				})
			}
		script.
			function initMap() {
				var map = new google.maps.Map(document.getElementById('map-dark'), {
				zoom: 15,
				center: {lat: -34.397, lng: 150.644}
				});
				var geocoder = new google.maps.Geocoder();
				geocodeAddress(geocoder, map);
				
			}
			function geocodeAddress(geocoder, resultsMap) {
				var address = document.getElementById('address').value;
				geocoder.geocode({'address': address}, function(results, status) {
				if (status === 'OK') {
				resultsMap.setCenter(results[0].geometry.location);
				var marker = new google.maps.Marker({
				map: resultsMap,
				position: results[0].geometry.location
				});
				} else {
				console.log('Geocode was not successful for the following reason: ' + status);
				}
				});
			}
		script(async='', defer='', src='https://maps.googleapis.com/maps/api/js?key=AIzaSyBnLyu9KRX0dTheISKLkC2IUUBKeWD7TdU&callback=initMap')
		script.
			$(document).ready(function(){
				$("#paymentButton").click(function(){
					var x = $("#intReceiptNumber").val()
					var y = $("#intTicketID").val()
					$.post('/index/payment', {
						x: x,
						y: y,
					}).done(function(data){
						location.reload()
					})
				})
			})
		script.
			function trigs(ticketNumber){
				$(document).ready(function(){
					$("#generate").click(function(){
						window.open(`/index/receipt/` + $("#intTicketID").val())
					})




					$('#triggerButton').trigger('click');
					$.post('/index/ticket', {intUserViolationTicketID: ticketNumber})
					.done(function(data){
						if(data[0].isPaid == 0){
							$("#smaller").text("Status: Unpaid")
							$("#smaller").css("color", "red")
							$('#generate').hide()
							$('#paymentButton').show()
						}
						else if(data[0].isPaid == 3){
							$("#smaller").css("color", "yellow")
							$("#smaller").text("Status: Pending")
							$('#intReceiptNumber').val(data[0].intReceiptNumber)
							$('#intReceiptNumber').attr("disabled");
							$('#generate').hide()
							$('#paymentButton').hide()
						}
						else{
							
							$("#smaller").css("color", "green")
							$("#smaller").text("Status: Paid")
							$('#intReceiptNumber').val(data[0].intReceiptNumber)
							$('#intReceiptNumber').attr("disabled");
							$('#paymentButton').hide()
							$('#generate').show()
						}
						$('#intTicketID').val(data[0].intTicketID)
						$('#strLicenseNumber').val(data[0].strTicketLicenseNo)
						$('#strLicenseName').val(data[0].strLicenseFirstName+' '+data[0].strLicenseLastName)
						$('#strPlateNumber').val(data[0].strTicketPlateNumber)
						$('#strEnforcerName').val(data[0].strAuthorityFirstName+' '+data[0].strAuthorityLastName)
						$('#datDate').val(data[0].strTicketDate)
						$('#address').val(data[0].strTicketAddress)
						var violength = data.length;
						var sum = 0;
						$('#tbody').empty()
						for(var i = 0; i<violength; i++){
							sum = sum + data[i].intCharge;
							$('#tbody').append(`<tr><td class="title-h bg-primary">`+data[i].strSection+`</td><td>`+data[i].strViolation+`</td><td>`+data[i].intCharge+`</td></tr>`)
						}
						$('#tbody').append(`<tr><td class="title-h bg-primary">Total</td><td></td><td>`+sum+`</td></tr>`)
						initMap();

					})
				});
			}
		
