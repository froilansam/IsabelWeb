			function puta(){
				var pages = $('.showPage').val()
					alert(pages);
					var ticket = {}
					var ticketLength =  null;
					var filter = $('#filter').val();
					$.post('/index/violation', {isPaid: filter})
					.done(function(data){
						ticket = data.violations;
						ticketLength = ticket.length;
					alert(pages);
					$('#pagination-demo').twbsPagination('destroy');
					$('#pagination-demo').twbsPagination($.extend({}, {
						totalPages: Math.ceil(ticketLength/pages),
						visiblePages: 5,
						onPageClick: function (event, page) {
							alert('e1')
							$.post('/index/vio', {intPage: pages, intStart: ((page-1)*pages), isPaid: filter, sortie: $('#sort').val()})
							.done(function(data){
								alert('entered2')
								var vio = data.vio;
								$("#ticketStatus").empty()
								alert(pages);
								for(var i = 0; i < vio.length; i++){
									alert(JSON.stringify(vio[i]));
									$("#ticketStatus").append(`<div class="row"><div class="col-md-3"></div><div class="col-lg-6"><div onclick='trigs(`+vio[i].intUserViolationTicketID+`)' class="form-group form-grouped focused"><label>Ticket Number: `+vio[i].intUserViolationTicketID+`</label><small>`+vio[i].strTicketDate+`</small><input disabled="" style="cursor: pointer;" type="text" value="Full Name: `+vio[i].strLicenseFirstName+` `+vio[i].strLicenseLastName+`" class="form-control"/><input disabled="" style="cursor: pointer;" type="text" value="License Number: `+vio[i].strLicenseLicenseNo+`" class="form-control"/></div></div><div class="col-md-3"></div></div>`);
								}
								
							})
						}
					}));
						
						
							
					})
			}

			function puta2(){
					var pages = $('.showLicensePage').val()
				
					alert(pages);
					var license = {}
					var licenseLength =  null;
					var filter = $('#filterLicense').val();
					$.post('/admin/license1', {status: filter})
					.done(function(data){
						console.log(JSON.stringify(data.license))
						license = data.license;
						licenseLength = license.length;
					alert(pages);
					$('#pagination-demo2').twbsPagination('destroy'); 
					$('#pagination-demo2').twbsPagination($.extend({}, {
						totalPages: Math.ceil(licenseLength/pages),
						visiblePages: 5,
						onPageClick: function (event, page) {
							alert('e1')
							$.post('/admin/license2', {intPage: pages, intStart: ((page-1)*pages), status: filter, sortie: $('#sortLicense').val()})
							.done(function(data){
								var lisensya = data.lisensya;
								$("#licenseStatus").empty();
								for(var i = 0; i < lisensya.length; i++){
									alert(JSON.stringify(lisensya[i]));
									$("#licenseStatus").append(`<div class="row"><div class="col-md-3"></div><div class="col-lg-6"><div class="form-group form-grouped focused"><label>License Number: `+lisensya[i].strTicketLicenseNo+`</label><small>Status: On Hold</small><input disabled="" style="cursor: pointer;" type="text" value="Full Name: `+lisensya[i].strLicenseFirstName+` `+lisensya[i].strLicenseLastName+`" class="form-control"/></div></div><div class="col-md-3"></div></div>`);
								}
								
							})
						}
					}));
						
						
							
					})
			}

