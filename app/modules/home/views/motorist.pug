			function license(strLicenseNoParam){
				alert(strLicenseNoParam);
				$.post('/admin/motorist', {strLicenseNo: strLicenseNoParam})
				.done(function(data){
					
					var dmotor = data.motor;
					alert(JSON.stringify(dmotor.strLicenseNo))
					$('#strLicenseNo').val(dmotor.strLicenseNo);
					if(dmotor.booLicenseType == 1)
						$('#strLicenseType').val("Non-Professional")
					else
						$('#strLicenseType').val("Professional")
					if(dmotor.strLicenseMiddleName != null || dmotor.strLicenseMiddleName != "")
						$('#strLicenseName').val(dmotor.strLicenseFirstName+' '+dmotor.strLicenseMiddleName+' '+dmotor.strLicenseLastName)
					else
						$('#strLicenseName').val(dmotor.strLicenseFirstName+' '+dmotor.strLicenseLastName)
					$('#strLicenseSex').val(dmotor.strLicenseSex)
					$('#datLicenseBirthDate').val(dmotor.datLicenseBirthDate)
					$('#strLicenseNationality').val(dmotor.strLicenseNationality)
					$('#intLicenseWeight').val(dmotor.intLicenseWeight+" lbs")
					$('#intLicenseHeight').val(dmotor.intLicenseHeight+" cm")
					if(dmotor.intLicenseRestrictions == 1){
						$("#strLicenseRestriction").val(`Motorbikes or motorized tricycles`)
					}
					else if(dmotor.intLicenseRestrictions == 2){
						$("#small").text(`GVW - Gross Vehicle Weight`);
						$("#strLicenseRestriction").val(`Motor vehicle up to 4500 kg GVW`)
					}
					else if(dmotor.intLicenseRestrictions == 3){
						$("#small").text(`GVW - Gross Vehicle Weight`);
						$("#strLicenseRestriction").val(`Motor vehicle above 4500 kg GVW`)
					}
					else if(dmotor.intLicenseRestrictions == 4){
						$("#small").text(`GVW - Gross Vehicle Weight`);
						$("#strLicenseRestriction").val(`Automatic transmission up to 4500 kg GVW`)
					}
					else if(dmotor.intLicenseRestrictions == 5){
						$("#small").text(`GVW - Gross Vehicle Weight`);
						$("#strLicenseRestriction").val(`Automatic transmission above 4500 kg GVW`)
					}
					else if(dmotor.intLicenseRestrictions == 6){
						$("#small").text(`GVW - Gross Vehicle Weight`);
						$("#strLicenseRestriction").val(`Articulated vehicle 1600 KGS GVW and below`)
					}
					else if(dmotor.intLicenseRestrictions == 7){
						$("#small").text(`GVW - Gross Vehicle Weight`);
						$("#strLicenseRestriction").val(`Articulated vehicle 1601 up to 4500 KGS GVW`)
					}
					else if(dmotor.intLicenseRestrictions == 8){
						$("#small").text(`GVW - Gross Vehicle Weight`);
						$("#strLicenseRestriction").val(`Articulated vehicle 4501 kgs and above GVW`)
					}
					if(dmotor.strLicenseCondition == 'A' || dmotor.strLicenseCondition == 'a')
						$("#strLicenseCondition").val(`Wear eyeglasses`)
					else if(dmotor.strLicenseCondition == 'B' || dmotor.strLicenseCondition == 'b')
						$("#strLicenseCondition").val(`Drive only with special equipment for upper limbs`)
					else if(dmotor.strLicenseCondition == 'C' || dmotor.strLicenseCondition == 'c')
						$("#strLicenseCondition").val(`Drive only with special equipment for lower limbs`)
					else if(dmotor.strLicenseCondition == 'D' || dmotor.strLicenseCondition == 'd')
						$("#strLicenseCondition").val(`Daylight driving only`)
					else if(dmotor.strLicenseCondition == 'E' || dmotor.strLicenseCondition == 'e')
						$("#strLicenseCondition").val(`Accompanied by a person with normal hearing`)
					else if(dmotor.strLicenseCondition == '' || dmotor.strLicenseCondition == null)
						$("#strLicenseCondition").val(`None`)
					$("#address").val(dmotor.strLicenseAddress)
					$('#buttonesTrig').trigger('click');
				})
			}
